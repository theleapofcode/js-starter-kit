{
  "name": "js-starter-kit",
  "version": "0.8.0",
  "description": "Starter kit for JS project",
  "scripts": {
    "nsp:check": "nsp check",
    "srcserver:start": "babel-node buildScripts/srcServer.js",
    "start": "npm-run-all --parallel nsp:check srcserver:start lint:watch test:watch",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel start localtunnel",
    "lint": "esw webpack.config.*, src, buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theleapofcode/js-starter-kit.git"
  },
  "keywords": [
    "starter-kit",
    "js"
  ],
  "author": "theleapofcode",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/theleapofcode/js-starter-kit/issues"
  },
  "homepage": "https://github.com/theleapofcode/js-starter-kit#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-latest": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "cheerio": "^0.22.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-watch": "^2.1.14",
    "express": "^4.14.0",
    "jsdom": "^9.9.1",
    "localtunnel": "^1.8.2",
    "mocha": "^3.2.0",
    "npm-run-all": "^3.1.2",
    "nsp": "^2.6.2",
    "open": "0.0.5",
    "style-loader": "^0.13.1",
    "voca": "^1.1.0",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0"
  }
}
